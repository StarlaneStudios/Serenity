{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist"]
		},
		"publish": {
			"dependsOn": ["^publish"],
			"outputs": ["dist"]
		},
		"test": {
			"cache": true,
			"persistent": true
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}